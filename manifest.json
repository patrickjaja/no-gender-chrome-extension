{
  "manifest_version": 3,
  "name": "NoGender",
  "version": "1.0.0",
  "description": "Entfernt gendered Texte auf allen Webseiten.",
  "permissions": ["activeTab"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
